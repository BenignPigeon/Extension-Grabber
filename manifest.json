{
  "manifest_version": 3,
  "name": "Extension Grabber",
  "version": "2.0",
  "description": "Generates an HTML file with links to all installed Chrome extensions.",
  "permissions": ["management", "downloads", "storage"],
  "action": {
    "default_popup": "html/main-popup.html",
    "default_title": "Generate Extension Links"
  },
  "icons": {
    "16": "image/icon.png",
    "48": "image/icon.png",
    "128": "image/icon.png"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
